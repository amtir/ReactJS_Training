var a = [2,4,5,6,7,9]
a.map((data) => {return data*2})
[4, 8, 10, 12, 14, 18]

var a = [2,4,5,6,7,9,1,6,3,8]
a.filter((data) => {return data>5})
[6, 7, 9, 6, 8]

var a = [2,4,5,6,7,9]
a.map((data) => {return data>2})
[false, true, true, true, true, true]

var a = ["Delhi","Venice","Amsterdam","Innsburg"]

a.indexOf('Delhi')
0
a.indexOf('Innsburg')
3
a.indexOf('Boston')
-1
a.indexOf('Cincinnati')
-1


